{
  "gitbook": "3.x.x",
  "plugins": [
    "include-codeblock",
    "ace",
    "uml",
    "mathjax@https://github.com/OrgVue/gitbook-plugin-mathjax.git#speech-fix",
    "collapsible-menu-ex",
    "ga"
  ],
  "pluginsConfig": {
    "uml": {
      "format": "png",
      "nailgun": false
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "theme": "monokai"
    },
    "mathjax": {
      "forceSVG": true
    },
    "ga": {
          "token": "UA-131534053-1"
    }
  }
}