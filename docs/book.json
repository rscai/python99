{
  "gitbook": "3.x.x",
  "plugins": [
    "include-codeblock",
    "ace",
    "uml",
    "mathjax",
    "collapsible-menu-ex",
    "analytics"
  ],
  "pluginsConfig": {
    "uml": {
      "format": "png",
      "nailgun": false
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "theme": "monokai"
    },
    "analytics": {
      "google": "UA-131534053-1"
    }
  }
}